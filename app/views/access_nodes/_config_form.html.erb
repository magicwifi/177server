<%= form_tag "/access_nodes/setconfig" do  %>
  <%= hidden_field_tag "id", @access.id %>
  <div class="field">
    <%= select_tag :cmdline, "<option value='60'>重启设备</option><option value='120'>无线关闭</option><option value='180'>无线开启</option><option value='240'>无线重启</option>".html_safe %>
  </div>
  <%= submit_tag "远程管理", class: "btn btn-large btn-primary" %>
<% end %>
